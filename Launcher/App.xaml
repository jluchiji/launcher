<Application x:Class="Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
        
            <ResourceDictionary.MergedDictionaries>
                <!-- Common theme resources, such as control templates -->
                <ResourceDictionary x:Name="CommonResources" Source="Themes/Theme.Common.xaml"></ResourceDictionary>
                <!-- Current theme resources, such as brushes -->
                <ResourceDictionary x:Name="ThemeResources" Source="Themes/Theme.Dark.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            
            
        </ResourceDictionary> 
       
    </Application.Resources>
</Application>
